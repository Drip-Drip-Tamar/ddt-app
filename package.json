{
    "name": "astro-sanity-starter",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "astro dev",
        "start": "astro dev",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro",
        "create-project": "node ./sanity-export/create-project.js",
        "import": "node ./sanity-export/import.js",
        "export": "node ./sanity-export/export.js",
        "import-content": "node ./sanity-export/import-content.js",
        "test": "vitest run",
        "test:unit": "vitest run tests/unit",
        "test:integration": "vitest run tests/integration",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "lint": "eslint .",
        "typecheck": "astro check && tsc --noEmit",
        "test:all": "npm run lint && npm run typecheck && npm run build && npm run test"
    },
    "dependencies": {
        "@astrojs/netlify": "^6.5.9",
        "@astrojs/react": "^4.3.0",
        "@sanity/astro": "^3.1.6",
        "@sanity/client": "^7.10.0",
        "@sanity/image-url": "^1.2.0",
        "@sanity/validation": "^3.14.4",
        "@sanity/visual-editing": "^3.0.3",
        "@tailwindcss/vite": "^4.0.9",
        "astro": "^5.1.1",
        "astro-icon": "^1.1.0",
        "chart.js": "^4.5.0",
        "chartjs-plugin-annotation": "^3.1.0",
        "dotenv": "^16.4.7",
        "fs-extra": "^11.2.0",
        "groq": "^4.6.0",
        "marked": "^15.0.4",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "tailwindcss": "^4.0.9"
    },
    "devDependencies": {
        "@astrojs/check": "^0.9.4",
        "@sanity/export": "^3.38.1",
        "@sanity/import": "^3.37.4",
        "@stackbit/cms-sanity": "^0.2.86",
        "@stackbit/types": "^2.0.0",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@types/fs-extra": "^11.0.4",
        "@typescript-eslint/eslint-plugin": "^8.43.0",
        "@typescript-eslint/parser": "^8.43.0",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "configstore": "7.0.0",
        "daisyui": "^5.0.0",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-astro": "^1.3.1",
        "jsdom": "^26.1.0",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "overrides": {
        "rollup": "npm:@rollup/wasm-node"
    }
