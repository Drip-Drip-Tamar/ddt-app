{
    "name": "astro-sanity-starter",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "astro dev",
        "start": "astro dev",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro",
        "create-project": "node ./sanity-export/create-project.js",
        "import": "node ./sanity-export/import.js",
        "export": "node ./sanity-export/export.js",
        "import-content": "node ./sanity-export/import-content.js",
        "test": "vitest run",
        "test:unit": "vitest run tests/unit",
        "test:integration": "vitest run tests/integration",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "lint": "eslint .",
        "typecheck": "astro check && tsc --noEmit",
        "test:all": "npm run lint && npm run typecheck && npm run build && npm run test"
    },
    "dependencies": {
        "@astrojs/netlify": "^6.6.4",
        "@astrojs/react": "^4.4.2",
        "@sanity/astro": "^3.1.6",
        "@sanity/client": "^7.14.1",
        "@sanity/image-url": "^2.0.3",
        "@sanity/validation": "^3.14.4",
        "@sanity/visual-editing": "^5.2.1",
        "@tailwindcss/vite": "^4.0.9",
        "astro": "^5.17.1",
        "astro-icon": "^1.1.0",
        "chart.js": "^4.5.0",
        "chartjs-plugin-annotation": "^3.1.0",
        "dotenv": "^17.2.4",
        "fs-extra": "^11.2.0",
        "groq": "^5.8.1",
        "marked": "^17.0.1",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "tailwindcss": "^4.0.9"
    },
    "devDependencies": {
        "@astrojs/check": "^0.9.6",
        "@sanity/export": "^6.0.5",
        "@sanity/import": "^4.1.0",
        "@stackbit/cms-sanity": "^0.2.92",
        "@stackbit/types": "^2.0.0",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.2",
        "@types/fs-extra": "^11.0.4",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@typescript-eslint/parser": "^8.54.0",
        "@vitest/coverage-v8": "^4.0.18",
        "@vitest/ui": "^4.0.18",
        "configstore": "^8.0.0",
        "daisyui": "^5.5.18",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-astro": "^1.3.1",
        "jsdom": "^28.0.0",
        "typescript": "^5.9.2",
        "vitest": "^4.0.18"
    },
    "overrides": {
        "rollup": "npm:@rollup/wasm-node"
    }
